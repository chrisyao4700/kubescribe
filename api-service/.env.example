OPSLOG_DB_URL=postgresql+psycopg://opslog:change-me@localhost:5432/opslog
# REPORT_DB_URL=postgresql+psycopg://user:pass@report-host:5432/report

# If set, required via header x-api-key
# API_KEY=dev-secret

# When running locally (out-of-cluster), set namespace explicitly
KUBE_NAMESPACE=default

# The image to run for core-runner Jobs
JOB_IMAGE=your-registry/kubescribe/core-runner:0.1.0
JOB_ENV_FROM_SECRET=core-runner-secrets
JOB_BACKOFF_LIMIT=0
JOB_TTL_SECONDS=600
